    Unboxed.Rep.Tuple1.Word8,
    Unboxed.Rep.Tuple1.Word16,
    Unboxed.Rep.Tuple1.Word32,
    Unboxed.Rep.Tuple1.Word64,
    Unboxed.Rep.Tuple2.Word.Unlifted,
    Unboxed.Rep.Sum2.Word.Unlifted

